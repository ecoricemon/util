[ default ]
subjectAltName = @alt_names

[ var ]
net = your.IP.subnet

[ alt_names ]
DNS.1 = your.domain.name
DNS.2 = *.your.domain.name
IP.1 = $var::net.2
IP.2 = $var::net.3
IP.3 = $var::net.4
IP.4 = $var::net.5
IP.5 = $var::net.6
IP.6 = $var::net.7
IP.7 = $var::net.8
IP.8 = $var::net.9
IP.9 = $var::net.10
